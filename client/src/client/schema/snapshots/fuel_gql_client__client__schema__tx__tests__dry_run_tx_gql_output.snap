---
source: fuel-client/src/client/schema/tx.rs
expression: query.query
---
mutation($tx: HexString!, $utxoValidation: Boolean) {
  dryRun(tx: $tx, utxoValidation: $utxoValidation) {
    param1
    param2
    amount
    assetId
    gas
    digest
    contract {
      id
    }
    is
    pc
    ptr
    ra
    rb
    rc
    rd
    reason
    receiptType
    to {
      id
    }
    toAddress
    val
    len
    result
    gasUsed
    data
    messageId
    sender
    recipient
    nonce
    contractId
  }
}


