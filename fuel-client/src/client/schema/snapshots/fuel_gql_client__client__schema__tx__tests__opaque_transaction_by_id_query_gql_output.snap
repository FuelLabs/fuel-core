---
source: fuel-client/src/client/schema/tx.rs
expression: operation.query
---
query($id: TransactionId!) {
  transaction(id: $id) {
    rawPayload
    receipts {
      rawPayload
    }
    status {
      __typename
      ... on SubmittedStatus {
        time
      }
      ... on SuccessStatus {
        block {
          id
        }
        time
        programState {
          returnType
          data
        }
      }
      ... on SqueezedOutStatus {
        reason
      }
      ... on FailureStatus {
        block {
          id
        }
        time
        reason
        programState {
          returnType
          data
        }
      }
    }
  }
}


