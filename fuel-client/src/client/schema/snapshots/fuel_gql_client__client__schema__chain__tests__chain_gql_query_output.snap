---
source: fuel-client/src/client/schema/chain.rs
expression: operation.query
---
query Query {
  chain {
    baseChainHeight
    name
    peerCount
    latestBlock {
      id
      header {
        id
        daHeight
        transactionsCount
        outputMessagesCount
        transactionsRoot
        outputMessagesRoot
        height
        prevRoot
        time
        applicationHash
      }
      transactions {
        id
      }
    }
    consensusParameters {
      contractMaxSize
      maxInputs
      maxOutputs
      maxWitnesses
      maxGasPerTx
      maxScriptLength
      maxScriptDataLength
      maxStorageSlots
      maxPredicateLength
      maxPredicateDataLength
      gasPriceFactor
      gasPerByte
      maxMessageDataLength
    }
  }
}

