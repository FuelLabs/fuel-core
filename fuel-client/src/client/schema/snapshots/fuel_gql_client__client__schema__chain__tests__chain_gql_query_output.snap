---
source: fuel-client/src/client/schema/chain.rs
expression: operation.query
---
query Query {
  chain {
    baseChainHeight
    name
    peerCount
    latestBlock {
      height
      id
      time
      producer
      transactions {
        id
      }
    }
    transactionParameters {
      contractMaxSize
      maxInputs
      maxOutputs
      maxWitnesses
      maxGasPerTx
      maxScriptLength
      maxScriptDataLength
      maxStaticContracts
      maxStorageSlots
      maxPredicateLength
      maxPredicateDataLength
    }
  }
}

