---
source: crates/client/src/client/schema/block.rs
expression: operation.query
---
query($after: String, $before: String, $first: Int, $last: Int) {
  blocks(after: $after, before: $before, first: $first, last: $last) {
    edges {
      cursor
      node {
        id
        header {
          id
          daHeight
          transactionsCount
          messageReceiptCount
          transactionsRoot
          messageReceiptRoot
          height
          prevRoot
          time
          applicationHash
        }
        consensus {
          __typename
          ... on Genesis {
            chainConfigHash
            coinsRoot
            contractsRoot
            messagesRoot
          }
          ... on PoAConsensus {
            signature
          }
        }
        transactions {
          id
        }
      }
    }
    pageInfo {
      endCursor
      hasNextPage
      hasPreviousPage
      startCursor
    }
  }
}


