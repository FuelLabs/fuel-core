---
source: crates/client/src/client/schema/chain.rs
expression: operation.query
---
query {
  chain {
    baseChainHeight
    name
    peerCount
    latestBlock {
      id
      header {
        id
        daHeight
        transactionsCount
        messageReceiptCount
        transactionsRoot
        messageReceiptRoot
        height
        prevRoot
        time
        applicationHash
      }
      consensus {
        __typename
        ... on Genesis {
          chainConfigHash
          coinsRoot
          contractsRoot
          messagesRoot
        }
        ... on PoAConsensus {
          signature
        }
      }
      transactions {
        id
      }
    }
    consensusParameters {
      txParams {
        maxInputs
        maxOutputs
        maxWitnesses
        maxGasPerTx
      }
      predicateParams {
