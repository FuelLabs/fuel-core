---
source: crates/client/src/client/schema/tx.rs
expression: query.query
---
mutation($txs: [HexString!]!, $utxoValidation: Boolean, $gasPrice: U64) {
  dryRun(txs: $txs, utxoValidation: $utxoValidation, gasPrice: $gasPrice) {
    id
    status {
      __typename
      ... on DryRunSuccessStatus {
        programState {
          returnType
          data
        }
        receipts {
          param1
          param2
          amount
          assetId
          gas
          digest
          id
          is
          pc
          ptr
          ra
          rb
          rc
          rd
          reason
          receiptType
          to
          toAddress
          val
          len
          result
          gasUsed
          data
          sender
          recipient
          nonce
          contractId
          subId
        }
        totalGas
        totalFee
      }
      ... on DryRunFailureStatus {
        programState {
          returnType
          data
        }
        receipts {
          param1
          param2
          amount
          assetId
          gas
          digest
          id
          is
          pc
          ptr
          ra
          rb
          rc
          rd
          reason
          receiptType
          to
          toAddress
          val
          len
          result
          gasUsed
          data
          sender
          recipient
          nonce
          contractId
          subId
        }
        totalGas
        totalFee
      }
    }
  }
}
