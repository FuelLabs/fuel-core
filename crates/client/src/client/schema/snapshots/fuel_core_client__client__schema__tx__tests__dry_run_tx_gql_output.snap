---
source: crates/client/src/client/schema/tx.rs
expression: query.query
---
mutation($txs: [HexString!]!, $utxoValidation: Boolean) {
  dryRun(txs: $txs, utxoValidation: $utxoValidation) {
    param1
    param2
    amount
    assetId
    gas
    digest
    contract {
      id
    }
    is
    pc
    ptr
    ra
    rb
    rc
    rd
    reason
    receiptType
    to {
      id
    }
    toAddress
    val
    len
    result
    gasUsed
    data
    sender
    recipient
    nonce
    contractId
    subId
  }
}


