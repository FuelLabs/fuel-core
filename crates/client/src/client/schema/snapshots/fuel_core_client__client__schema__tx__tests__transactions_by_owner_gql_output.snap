            totalFee
            resolvedOutputs {
              utxoId
              output {
                __typename
                ... on CoinOutput {
                  to
                  amount
                  assetId
                }
                ... on ContractOutput {
                  inputIndex
                  balanceRoot
                  stateRoot
                }
                ... on ChangeOutput {
